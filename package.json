{
  "name": "epilogue",
  "description": "Create REST resources and controllers with Sequelize and Express",
  "version": "0.3.1",
  "author": "David Chester <dchester@shutterstock.com>",
  "main": "lib/",
  "scripts": {
    "jshint": "jshint ./lib ./tests",
    "mocha": "mocha --check-leaks -R spec -u exports tests/*.js",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -- tests/*.js",
    "test": "npm run-script jshint && npm run-script mocha && npm run-script istanbul"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dchester/epilogue.git"
  },
  "keywords": [
    "sequelize",
    "sequelizejs",
    "rest",
    "restful",
    "epilogue",
    "json"
  ],
  "devDependencies": {
    "sqlite3": "~2.2.0",
    "request": "~2.34.0",
    "express": "3.x",
    "restify": "~2.8.3",
    "mocha": ">=1.17.0",
    "chai": ">= 1.9.0",
    "jshint": ">=2.4.2",
    "sequelize": "~1.7.0-rc6",
    "istanbul": "~0.1.45"
  },
  "dependencies": {
    "async": "~0.2.10",
    "lodash": "~2.4.1"
  }
}
